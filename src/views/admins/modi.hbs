    {{#each peticionid}}
    <div class="container p-4">
        <div class="row">
            <div class="col-md-4">
                <div class="card">
                    <div class="car-bady">
                        <form action="/admins/modificaar/peticiones/{{id_peticiones}}" method="post">
                            <div class="form-group">
                                <label for="">modifcar</label>
                            </div>
                            <div class="form-group">
                                <input class="form-control" value="{{nombre}}" name="nombre"
                                    placeholder=" ingresar edad" id="">
                                <input class="form-control" value="{{genero}}" name="genero"
                                    placeholder=" ingresar edad" id="">
                                <input class="form-control" value="{{album}}" name="album" placeholder=" ingresar edad"
                                    id="">
                                <div class="form-group">
                                    <button type="submit" value="{{id_peticiones}}" class="btn btn-success btn-block">
                                        Enviar</button>
                                </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>


        {{/each}}

